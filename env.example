# AWS 설정
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Device Farm 설정 (자동으로 설정됨)
DEVICEFARM_PROJECT_ARN=arn:aws:devicefarm:us-west-2:123456789012:project:your-project-id
DEVICEFARM_DEVICE_POOL_ARN=arn:aws:devicefarm:us-west-2:123456789012:devicepool:your-device-pool-id

# Device Farm 서버 설정 (자동으로 설정됨)
DEVICEFARM_APPIUM_SERVER_URL=127.0.0.1
DEVICEFARM_APPIUM_SERVER_PORT=4723
DEVICEFARM_DEVICE_PLATFORM=Android

# Applitools 설정
APPLITOOLS_API_KEY=your_applitools_api_key_here
